public class Frequency
{
  public static void main(String[] args)
  {
    char[] str={'h','e','l','l','o','\0'};    
    int freq=0;
    int i;
    char  ch='l';
    int length=0;
    for(i=0;str[i]!='\0';i++)
     {
	length++;
     }
    for(i=0;i<length;i++)
    {
	if(str[i]==ch) 
         {       
            freq++;
          }
    }    
    System.out.println("frequency of l is " + freq);
 }  	
}
