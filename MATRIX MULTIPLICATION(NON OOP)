public class Matrix
{
   public static void  main(String[] args)
   {
     int i,j,k;
     int a[][]={{3,1,4},{8,1,4},{4,5,1}};
     int b[][]={{6,1,3},{1,6,4},{5,4,6}};
     int c[][]=new int[3][3];
     for(i=0;i<3;i++)
     {
        for(j=0;j<3;j++)
        {
           c[i][j]=0;
           for(k=0;k<3;k++)
           {
              c[i][j]+=a[i][k]*b[k][j];
           }
           System.out.print(" " + c[i][j]);
        }
      System.out.print("\n");
     }  
  }
}
